{
	"name": "kudu-components",
	"version": "0.0.4",
	"description": "Just a demo",
	"author": {
		"name": "Kudu Consultant LLC",
		"email": "services@kudu.design",
		"url": "https://kudu.design"
	},
	"license": "GNU GPL",
	"type": "module",
	"keywords": [
		"kudu",
		"library components",
		"svelte",
		"svletekit"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/kudu-consultant/kudu-components"
	},
	"homepage": "https://github.com/kudu-consultant/kudu-components",
	"bugs": "https://github.com/kudu-consultant/kudu-components/issues",
	"files": [
		"LICENSE",
		"README.md",
		"package.json",
		"dist"
	],
	"publishConfig": {
		"access": "public"
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		},
		"./icons": {
			"types": "./dist/icons/index.d.ts",
			"svelte": "./dist/icons/index.js"
		},
		"./modals": {
			"types": "./dist/modals/index.d.ts",
			"svelte": "./dist/modals/index.js"
		}
	},
	"typesVersions": {
		">4.0": {
			"icons": [
				"./dist/icons/index.d.ts"
			],
			"modals": [
				"./dist/modals/index.d.ts"
			],
			"*": [
				"./dist/index.d.ts"
			]
		}
	},
	"scripts": {
		"install:git-hooks": "lefthook add pre-commit && lefthook add pre-push && lefthook add commit-msg",
		"uninstall:git-hooks": "lefthook uninstall",
		"dev": "vite dev",
		"package": "svelte-package",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write ."
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "2.1.0",
		"@sveltejs/kit": "1.20.1",
		"@sveltejs/package": "^2.1.0",
		"@typescript-eslint/eslint-plugin": "5.59.8",
		"@typescript-eslint/parser": "5.59.8",
		"eslint": "8.41.0",
		"eslint-config-prettier": "8.8.0",
		"eslint-plugin-svelte": "2.30.0",
		"eslint-plugin-unused-imports": "2.0.0",
		"lefthook": "^1.4.1",
		"postcss": "8.4.24",
		"prettier": "2.8.8",
		"prettier-plugin-svelte": "2.10.1",
		"svelte": "3.59.1",
		"svelte-check": "3.4.3",
		"svelte-preprocess-import-assets": "1.0.0",
		"svelte2tsx": "^0.6.19",
		"tailwindcss": "3.3.2",
		"tslib": "2.5.2",
		"typescript": "5.1.3",
		"vite": "4.3.9",
		"vite-plugin-node-polyfills": "0.9.0"
	},
	"dependencies": {
		"@wagmi/core": "^1.3.7",
		"kudu-ui-system": "0.1.17",
		"kudu-utils": "0.0.3",
		"svelte": "3.59.1",
		"viem": "^1.2.12"
	}
}
